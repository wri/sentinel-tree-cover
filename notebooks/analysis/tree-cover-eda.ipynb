    "                            options=translateoptions)\n",
    "        ds = None\n",
    "        print(f'Hansen after gdal translate: {rs.open(hans_outpath).shape}')\n",
    "        \n",
    "        # Resample hansen\n",
    "        hans_raster = rs.open(hans_raster)\n",
    "        height = int(hans_raster.height)\n",
    "        width = int(hans_raster.width)\n",
